<test>
  <sequence>resources/visual_tracking_dataset/bricks/ground_truth/fi-br-m1.avi.warps.sequence.xml</sequence>
  <algorithm>
    <motion_model>
      <name>Homography8P</name>
    </motion_model>
    <object_model>
      <name>ImageGray</name>
      <parameters>
<!--
        <param>
          <name>template_image</name>
          <type>image</type>
          <value>resources/visual_tracking_dataset/bricks/templates/fi-br-ld.avi.template.png</value>
        </param>
-->
        <param>
          <name>template_image_shape</name>
          <type>num_list</type>
          <value>50 100</value>
        </param>
        <param>
          <name>template_equalize</name>
          <type>bool</type>
          <value>True</value>
        </param>
      </parameters>
    </object_model>
    <cost_function>
      <name>L2ImagesInvComp</name>
      <parameters>
        <param>
          <name>show_debug_info</name>
          <type>bool</type>
          <value>False</value>
        </param>
      </parameters>
    </cost_function>
    <optimizer>
      <name>GaussNewton</name>
      <parameters>
        <param>
          <name>max_iter</name>
          <type>int</type>
          <value>20</value>
        </param>
        <param>
          <name>show_iter</name>
          <type>bool</type>
          <value>False</value>
        </param>
      </parameters>
    </optimizer>
  </algorithm>
</test>


